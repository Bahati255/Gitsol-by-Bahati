

{
  "name": "gitsol",
  "displayName": "GitSol",
  "description": "Push a local project to GitHub directly from VS Code.",
  "version": "1.0.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:gitsol.createAndPushRepo"
  ],
  "main": "extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitsol.createAndPushRepo",
        "title": "GitSol: Create and Push to GitHub"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npx tsc -p ."
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.75.0",
    "typescript": "^4.5.5",
    "axios": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
